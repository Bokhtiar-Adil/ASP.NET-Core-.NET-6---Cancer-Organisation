    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="TreatmentCenters.TC_name" class="control-label"></label>
                <input asp-for="TreatmentCenters.TC_name" class="form-control" />
                <span asp-validation-for="TreatmentCenters.TC_name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TreatmentCenters.TC_description" class="control-label"></label>
                <input asp-for="TreatmentCenters.TC_description" class="form-control" />
