            <div class="form-group">
                <label asp-for="TreatmentCenters.TC_address" class="control-label"></label>
                <input asp-for="TreatmentCenters.TC_address" class="form-control" />
                <span asp-validation-for="TreatmentCenters.TC_address" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TreatmentCenters.TC_contact" class="control-label"></label>
                <input asp-for="TreatmentCenters.TC_contact" class="form-control" />
